<h1> Here it is </h1>

<%= link_to "Add new item", new_grocerylist_path, :class => "btn btn-success" %>

<% @grocerylists.each do |grocerylist| %>
	<li><%= link_to grocerylist.description, grocerylist_path(grocerylist.id)%>
		<%= link_to "Edit", edit_grocerylist_path(grocerylist.id), :class => "btn btn-danger" %>
		<%= link_to "Delete", grocerylist, method: :delete, data: {confirm: "You sure?"}%>
	</li>

<% end %>

